{
  "name": "@ehrocks/react-native-swag-personal-app",
  "version": "1.479.0",
  "description": "SWAG personal app as a library",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "pretest": "patch-package",
    "test": "yarn pretest && yarn tsc:jest:check && bash ./scripts/test.sh",
    "test:ci": "yarn pretest  && jest --ci --config=\"./jest.config.ci.js\" --logHeapUsage --forceExit --reporters=default --reporters=jest-junit --silent",
    "lint:check": "eslint \"{src/**/*.{js,ts,tsx},src/**/graphql-documents/*.graphql}\"",
    "lint:fix": "eslint --fix \"{src/**/*.{js,ts,tsx},src/**/graphql-documents/*.graphql}\"",
    "prepare": "husky install",
    "build": "bob build",
    "clean:ios": "cd example && rm -rf ios/Build && rm -rf ios/Pods",
    "clean:node": "cd example && rm -rf node_modules",
    "clean:all:ios": "yarn clean:node && yarn clean:ios",
    "release": "semantic-release",
    "install:all": "yarn install && yarn install:mimic && yarn install:pods",
    "install:mimic": "cd example && yarn install",
    "install:bundle": "cd example && bundle install",
    "install:pods": "cd example/ios && bundle exec pod install && rm -rf .xcode.env.local",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.yml",
    "e2e:ios:build": "detox build -c ios.sim.release",
    "e2e:ios:build:debug": "detox build -c ios.sim.debug",
    "e2e:ios:test:debug": "detox test -c ios.sim.debug --headless",
    "e2e:ios:test:local": "E2E_PLATFORM=ios sh e2e/scripts/io_test.sh",
    "e2e:ios:test": "detox test -c ios.sim.release --headless",
    "e2e:android:build": "detox build -c android.emu.release --retries=1",
    "e2e:android:test:local": "E2E_PLATFORM=android sh e2e/scripts/io_test.sh",
    "e2e:android:test": "detox test -c android.emu.release",
    "auto:compress:images": "bash scripts/compress-image.sh",
    "example": "yarn --cwd example",
    "start": "yarn example start",
    "android": "yarn example android",
    "ios": "yarn example ios",
    "sonar-scanner": "sonar-scanner",
    "commitlint": "commitlint",
    "tsc:jest:check": "tsc --project tsconfig.jest.json"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "author": "eBenefits <duc.nguyenanh@employmenthero.com>",
